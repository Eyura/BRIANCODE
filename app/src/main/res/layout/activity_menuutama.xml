<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".menuutama">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="ScrollViewSize">
        <LinearLayout
            android:id="@+id/boxname"
            android:layout_width="250dp"
            android:layout_height="100dp"
            android:layout_marginTop="10sp"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginStart="10dp"
                android:text="HALLO"
                android:textSize="32sp"
                android:padding="10sp"
                android:fontFamily="@font/holiday"
                tools:ignore="HardcodedText" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginStart="20dp"
                android:text="Brian Nayaka"
                android:textSize="32sp"
                android:fontFamily="@font/holiday"
                tools:ignore="HardcodedText" />


        </LinearLayout>

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/profile_image"
            android:layout_width="96dp"
            android:layout_height="96dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/boxname"
            android:onClick="Onprofilclick"
            android:src="@drawable/profil"
            app:civ_border_color="#FF000000"
            app:civ_border_width="2dp"
            tools:ignore="RtlHardcoded,SpeakableTextPresentCheck" />

        <androidx.appcompat.widget.SearchView
            android:id="@+id/pencarian"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_below="@id/boxname"
            android:layout_marginStart="50dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="50dp"
            android:layout_marginBottom="50dp"
            android:background="@drawable/shape2">

        </androidx.appcompat.widget.SearchView>

        <LinearLayout
            android:id="@+id/menuview"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:layout_below="@id/pencarian">

            <ImageButton
                android:id="@+id/penerbangan"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginStart="30dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/shapebackground"
                android:contentDescription="TODO"
                android:src="@drawable/penerbangan"
                tools:ignore="ContentDescription,DuplicateSpeakableTextCheck,HardcodedText,SpeakableTextPresentCheck" />

            <ImageButton
                android:id="@+id/Hoteltiket"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginTop="20dp"
                android:layout_marginStart="20dp"
                android:background="@drawable/shapebackground"
                android:src="@drawable/bangunan"
                tools:ignore="SpeakableTextPresentCheck" />

            <ImageButton
                android:id="@+id/Other1"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginTop="20dp"
                android:layout_marginStart="20dp"
                android:src="@drawable/gunung"
                android:background="@drawable/shapebackground"
                tools:ignore="SpeakableTextPresentCheck"
                android:importantForAccessibility="no">
            </ImageButton>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/menuname"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_below="@id/menuview">

            <TextView
                android:id="@+id/Flight"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="FLIGHT"
                android:textStyle="bold"
                android:layout_marginEnd="20dp"
                android:layout_weight="1"
                tools:ignore="HardcodedText" />

            <TextView
                android:id="@+id/Hotel"
                android:layout_width="285dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="HOTEL"
                android:textStyle="bold"
                tools:ignore="HardcodedText,TextSizeCheck" />
            <TextView
                android:id="@+id/other"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textStyle="bold"
                android:layout_marginStart="10dp"
                android:text="Other"
                android:layout_weight="1"
                tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/menuname"
            android:orientation="vertical">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_margin="20dp"
                android:background="@drawable/shapebackground"
                android:contentDescription="TODO"
                android:onClick="iklanOnClick"
                android:padding="10dp"
                android:src="@drawable/iklan1"
                tools:ignore="ContentDescription,HardcodedText,DuplicateSpeakableTextCheck,ImageContrastCheck" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_margin="20dp"
                android:background="@drawable/shapebackground"
                android:onClick="iklanOnClick"
                android:padding="10dp"
                android:src="@drawable/iklan2"
                android:contentDescription="TODO"
                tools:ignore="ContentDescription,HardcodedText">
            </ImageView>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_margin="20dp"
                android:background="@drawable/shapebackground"
                android:contentDescription="TODO"
                android:onClick="iklanOnClick"
                android:src="@drawable/iklan1"
                tools:ignore="ContentDescription,HardcodedText" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_margin="20dp"
                android:background="@drawable/shapebackground"
                android:onClick="iklanOnClick"
                android:padding="10dp"
                android:src="@drawable/iklan2"
                android:contentDescription="TODO"
                tools:ignore="ContentDescription,HardcodedText">
            </ImageView>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_margin="20dp"
                android:background="@drawable/shapebackground"
                android:onClick="iklanOnClick"
                android:src="@drawable/iklan1"
                android:contentDescription="TODO"
                tools:ignore="ContentDescription,HardcodedText">
            </ImageView>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_margin="20dp"
                android:background="@drawable/shapebackground"
                android:onClick="iklanOnClick"
                android:padding="10dp"
                android:src="@drawable/iklan2"
                android:contentDescription="TODO"
                tools:ignore="ContentDescription,HardcodedText">
            </ImageView>

        </LinearLayout>
    </RelativeLayout>



</ScrollView>
